<div id='clearance' class='sign-up'>
  <h2>Sign up</h2>

  <%= form_for @user do |form| %>
    <%= render :partial => '/users/form', :object => form %>

    <div class='submit-field'>
      <%= form.submit %>
    </div>

    <div class='other-links'>
      Already have a username and password? <%= link_to t('.sign_in'), sign_in_path %> here.<br />
	  or use one of the outside acounts:<br \>
	  <%= render :partial => '/shared/authBar' %>
	  
    </div>
  <% end %>
</div>
